{
  "name": "eri",
  "version": "0.0.0",
  "description": "A minimal design system",
  "main": "dist",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && parcel build src/index.ts --no-minify --no-source-maps && tsc && BABEL_ENV=css babel dist --out-dir dist && node scripts/removeCssImports",
    "build:style-guide": "rm -rf build && parcel build docs/index.html -d build --no-source-maps",
    "format": "prettier --write \"**/*\"",
    "start": "parcel docs/index.html",
    "test": "run-p test:*",
    "test:format": "prettier --list-different \"**/*\"",
    "test:lint": "tslint -c tslint.json 'docs/**/*.ts*' 'src/**/*.ts*'",
    "test:types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benji6/eri.git"
  },
  "author": "benji6",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benji6/eri/issues"
  },
  "homepage": "https://github.com/benji6/eri#readme",
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@types/react-router-dom": "^4.3.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.10.1",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "semantic-release": "^15.9.16",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
